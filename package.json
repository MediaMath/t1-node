{
  "name": "@mediamath/terminalone",
  "description": "Connection Module for T1 API written in node.js",
  "license": "Apache-2.0",
  "version": "0.7.0",
  "engines": {
    "node": ">= 4.0.0"
  },
  "engine-strict": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/MediaMath/t1-node.git"
  },
  "main": "index.js",
  "devDependencies": {
    "chai": "^4.0.0",
    "chai-as-promised": "^7.0.0",
    "codacy-coverage": "^2.0.2",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "fast-deep-equal": "^1.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "sinon": "^2.3.7",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "babyparse": "^0.4.6",
    "bluebird": "^3.3.4",
    "json-refs": "3.0.0",
    "jsonschema": "^1.1.0",
    "querystring": "^0.2.0",
    "request-promise": "4.2.1",
    "simple-oauth2": "^1.3.0"
  },
  "nyc": {
    "exclude": [
      "lib/schema",
      "test"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha",
    "coverage-report": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "integration": "mocha integration-test",
    "posttest": "npm run lint"
  }
}
